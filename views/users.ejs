<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

	<ul>
		 <% users.forEach(function(user) { %>
		 	
		 		<li><%= user.name %> - <%= user.age %> - <%= user.nationality %> 
		 			<%if (user.birthdate != undefined) { %>
		 				- <%= user.birthdate.toLocaleDateString() %></li>
		 			<% }; %>
		 <% }); %>
	</ul>

  </body>
</html>
